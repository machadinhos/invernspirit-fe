{
  "name": "sveltekit-ssg-demo-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:preview": "vite build && vite preview",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --write --list-different . && eslint . --fix",
    "format": "prettier --write .",
    "prepare-smui-theme": "npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "prepare-husky": "husky"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.20",
    "svelte": "^5.0.0",
    "svelte-icons-pack": "^3.0.0",
    "tailwindcss": "^3.4.9",
    "vite": "^5.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "@smui-extra/accordion": "^8.0.0-alpha.0",
    "@smui/icon-button": "^8.0.0-alpha.0",
    "@smui/menu-surface": "^8.0.0-alpha.0",
    "@smui/textfield": "^8.0.0-alpha.0",
    "smui-theme": "^8.0.0-alpha.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0"
  }
}
